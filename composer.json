{
    "name": "konsulting/laravel-blockade",
    "description": "A simple block for your Laravel app to prevent access without a known code, and to force to https if you wish.",
    "license": "MIT",
    "authors": [
        {
            "name": "Keoghan Litchfield",
            "email": "keoghan@klever.co.uk"
        }
    ],
    "require": {
        "php": "^5.6 | ^7.0",
        "laravel/framework": "~5.3|~5.4|~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6|^6.0",
        "orchestra/testbench": "~3.0",
        "orchestra/testbench-browser-kit": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Konsulting\\Laravel\\Blockade\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konsulting\\Laravel\\Blockade\\BlockadeServiceProvider"
            ]
        }
    }
}
